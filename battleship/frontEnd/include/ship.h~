#ifndef _SHIP_H
#define _SHIP_H
#include <Ogre.h>
class Ship {
 private:
  char* _id;

 public:
  Ship(char* id);
  ~Ship();
  void paintShip(Ogre::SceneManager * _sceneManager,
                             Ogre::SceneNode * _parentNode,                             
                              Ogre::Vector3* position);
  
};





#endif
